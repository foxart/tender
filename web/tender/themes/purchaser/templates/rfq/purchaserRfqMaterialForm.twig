<div id="rfq-material-form" class="modal fade">
	<div class="modal-dialog">
		<div class="modal-content">
			<form class="bs-modal form-horizontal" name="rfq-material-form" method="post" action="#">
				<div class="modal-header">
					<button type="button" class="cancel close">
						<span>&times;</span>
					</button>
					<h3 class="modal-title"></h3>
				</div>
				<div class="modal-body">
					<div class="form-group form-group-sm">
						<div class="col-md-12 has-feedback">
							<label class="control-label hidden" for="material_fake">material_fake</label>
							<input id="material_fake" class="form-control hidden" name="material_fake">
							<div class="help-block"></div>
						</div>
					</div>
					<div class="form-group">
						<label class="col-md-3 control-label" for="material_name">Name</label>
						<div class="col-md-9 has-feedback">
							<input id="material_name" class="form-control" name="material_name" type="text">
							<span class="form-control-feedback"></span>
							<div class="help-block"></div>
						</div>
					</div>
					<div class="form-group">
						<label class="col-md-3 control-label" for="rfq_material_quantity">Quantity</label>
						<div class="col-md-9 has-feedback">
							<input id="rfq_material_quantity" class="form-control" name="rfq_material_quantity" type="text">
							<span class="form-control-feedback"></span>
							<div class="help-block"></div>
						</div>
					</div>
				</div>
				<div class="modal-footer">
					<button class="submit btn btn-primary" type="submit">Submit</button>
					<a class="cancel btn btn-default" href="#">Close</a>
				</div>
			</form>
		</div>
	</div>
</div>
