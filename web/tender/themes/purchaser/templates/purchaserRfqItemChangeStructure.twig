{#MODAL#}
<div class="page-header">
	<h1>Request for quotation</h1>
</div>
<div class="indent"></div>
<div class="panel panel-default">
	<div class="panel-heading">
		<a href="#rfq-item-collapse" class="btn-block" data-toggle="collapse">
			RFQ details
			<span class="pull-right glyphicon glyphicon-modal-window"></span>
		</a>
	</div>
	<div id="rfq-item-collapse" class="panel-body collapse">
		<h3>RFQ details</h3>
		{{ rfq_item_update_link }}
		<div class="indent"></div>
		<div id="rfq_item">
			{{ rfq_item }}
		</div>
	</div>
</div>
<div class="row">
	<div id="rfq_material_list" class="col-md-7">
		{{ rfq_material_list }}
	</div>
	<div id="rfq_company_list" class="col-md-5">
		{{ rfq_company_list }}
	</div>
</div>
<div>
	<hr/>
</div>
<div class="indent"></div>
<div>
	<a class="btn btn-sm btn-default pull-right" href="{{ rfq_item_change_url_back }}">Back</a>
</div>
