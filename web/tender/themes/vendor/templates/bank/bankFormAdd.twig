<div id="bank-form-add" class="modal fade">
	<div class="modal-dialog">
		<div class="modal-content">
			<form class="bs-modal form-horizontal" name="bank-form-add" method="post" action="#">
				<div class="modal-header">
					<button type="button" class="cancel close">
						<span>&times;</span>
					</button>
					<h3 class="modal-title">Add company bank details</h3>
				</div>
				<div class="modal-body">
					<div class="row">
						<div class="col-md-12">
							<div class="form-group form-group-sm">
								<label class="col-md-3 control-label" for="bank_key">key</label>
								<div class="col-md-9 has-feedback">
									<input id="bank_key" class="form-control" name="bank_key" type="text">
									<span class="form-control-feedback"></span>
									<div class="help-block"></div>
								</div>
							</div>
						</div>
						<div class="col-md-12">
							<div class="form-group form-group-sm">
								<label class="col-md-3 control-label" for="bank_account">account</label>
								<div class="col-md-9 has-feedback">
									<input id="bank_account" class="form-control" name="bank_account" type="text">
									<span class="form-control-feedback"></span>
									<div class="help-block"></div>
								</div>
							</div>
						</div>
						<div class="col-md-12">
							<div class="form-group form-group-sm">
								<label class="col-md-3 control-label" for="bank_holder">holder</label>
								<div class="col-md-9 has-feedback">
									<input id="bank_holder" class="form-control" name="bank_holder" type="text">
									<span class="form-control-feedback"></span>
									<div class="help-block"></div>
								</div>
							</div>
						</div>
					</div>
				</div>
				<div class="modal-footer">
					<button class="submit btn btn-primary" type="submit">Submit</button>
					<a class="cancel btn btn-default pull-right" href="#">Close</a>
				</div>
			</form>
		</div>
	</div>
</div>
