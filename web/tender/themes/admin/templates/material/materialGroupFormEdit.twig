<div id="material-group-form" class="modal fade">
	<div class="modal-dialog">
		<div class="modal-content">
			<form class="bs-modal form-horizontal" name="material-group-form-edit" method="post" action="#">
				<div class="modal-header">
					<button type="button" class="cancel close">
						<span>&times;</span>
					</button>
					<h3 class="modal-title"></h3>
				</div>
				<div class="modal-body">
					<div class="form-group">
						<label class="col-md-3 control-label" for="name">Name</label>
						<div class="col-md-9 has-feedback">
							<input id="name" class="form-control" name="material_group_name" type="text">
							<span class="form-control-feedback"></span>
							<div class="help-block"></div>
						</div>
					</div>
				</div>
				<div class="modal-footer">
					<button class="submit btn btn-primary" type="submit">Submit</button>
					<a class="cancel btn btn-default" href="#">Close</a>
				</div>
			</form>
		</div>
	</div>
</div>
