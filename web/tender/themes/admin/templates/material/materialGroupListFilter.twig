<div class="row">
	<div class="bs-form {{ material_group_list_filter_metadata }}">
		<form class="form-horizontal" name="material-group-list-filter" method="get">
			<div class="col-md-10">
				<div class="form-group form-group-sm">
					<label class="col-md-3 control-label" for="group">
						Group:
					</label>
					<div class="col-md-9 has-feedback">
						<input id="group" class="form-control" name="group" type="text">
						<span class="form-control-feedback"></span>
						<div class="help-block"></div>
					</div>
				</div>
			</div>
			<div class="col-md-2">
				<div class="form-group form-group-sm">
					<div class="col-md-12">
						<button class="btn btn-default btn-block btn-sm" type="submit">
							Filter
						</button>
					</div>
				</div>
			</div>
		</form>
	</div>
</div>
